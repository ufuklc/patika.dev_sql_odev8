---- 3.Basamak
UPDATE employee
SET
name = 'XXXX'
where id=1;

UPDATE employee
SET
name = 'Start with G'
where name LIKE 'G%';

UPDATE employee
SET
name = 'Start with A'
where name LIKE 'A%';

UPDATE employee
SET
name = 'Start with B'
where name LIKE 'B%';

UPDATE employee
SET
name = 'Start with C'
where name LIKE 'C%';


---- 4.Basamak
DELETE from employee
where id=33;

DELETE from employee
where name LIKE '%A';

DELETE FROM employee 
WHERE id IN (1, 2, 3);

DELETE from employee
where birthday = '2010-01-01';

DELETE from employee
where email LIKE 'a%';

